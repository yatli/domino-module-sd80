; ==================================================================

; DisableExclusive=0/1			エクスクルーシブデータの送信抑制
; DisableBankSelectLSB=0/1		バンクセレクトLSBの送信抑制
; DisableBankSelectMSB=0/1		バンクセレクトMSBの送信抑制
; DisableResetExclusive=0/1		GS Reset, GM ON, XG Reset?の送信抑制
; DefaultBankSelectLSB=n		バンクセレクトLSB=0を変更して送信
; MapSelect=n					バンクセレクトLSB=nに変更して送信(Roland専用)
; comment=						コメント（無視されます）
; MIDI=MIDIメッセージ			A,B両ポートにMIDIメッセージを送信
; MIDI_A=MIDIメッセージ			AポートにMIDIメッセージを送信
; MIDI_B=MIDIメッセージ			BポートにMIDIメッセージを送信
; GSToGMEmu=1					GS ExclusiveをGM用に変換
; XGtoGMEmu=1					XG のバンクセレクトを解釈
; PatchMap=ﾊﾟｯﾁﾏｯﾌﾟｾｸｼｮﾝ??	パッチマップを使用します。

[NoExclusive]
DisableExclusive=1

[GMPatch]
comment=強制的にセレクトバンクを無効にしてGMにする
DisableBankSelectLSB=1
DisableBankSelectMSB=1

[GMON]
comment=ＧＭ－ＯＮを送信
MIDI_A=M:GMON

[SD90NativeOn]
comment=Edirol SD-90 Native On
MIDI_A=M:NativeON

[GeneralInit]
comment=一般的な初期化
MIDI_A=R:0 W:200

[GeneralInitGS]
comment=一般的な初期化(GS用)
MIDI_A=M:GSReset

[GSReset]
comment=ＧＳ－ＲＥＳＥＴを送信
MIDI_A=M:GSReset

[88Mode1]
comment=88Mode1
MIDI_A=M:88Mode1

[XGReset]
comment=ＸＧ初期化
MIDI_A=M:XGReset

[55Map]
comment=GS-RESETを無視して、５５ＭＡＰに初期化
MIDI_A=C:55MAP W:20 C:PC0 W:20
DefaultBankSelectLSB=1
DisableBankSelectLSB=1
MapSelect=1

[88Map]
comment=GS-RESETを無視して、８８ＭＡＰに初期化
MIDI=C:88MAP W:20 C:PC0 W:20
DefaultBankSelectLSB=2
MapSelect=2

[88ProMap]
comment=GS-RESETを無視して、８８ＰＲＯＭＡＰに初期化
MIDI=C:88PROMAP W:20 C:PC0 W:20
DefaultBankSelectLSB=3
MapSelect=3

[8850Map]
comment=GS-RESETを無視して、８８５０ＭＡＰに初期化
MIDI=C:8850MAP W:20 C:PC0 W:20
DefaultBankSelectLSB=4
MapSelect=4

[55MapFor88]
comment=バンクセレクトLSBとGS-RESETを無視して、５５ＭＡＰに初期化
MIDI_A=C:55MAP W:50 C:BANKMSB0 W:50 C:PC0 W:50
DisableBankSelectLSB=1
DisableBankSelectMSB=1
DisableResetExclusive=1
MapSelect=1

[Force55Map]
comment=バンクセレクトLSBとGS-RESETを無視して、５５ＭＡＰに初期化
MIDI_A=C:55MAP W:50 C:BANKMSB0 W:50 C:PC0 W:50
DisableBankSelectLSB=1
DisableBankSelectMSB=1
DisableResetExclusive=1
MapSelect=1

[Force88Map]
comment=バンクセレクトLSBとGS-RESETを無視して、８８ＭＡＰに初期化
MIDI=C:88MAP W:20 C:BANKMSB0 W:20 C:PC0
DisableBankSelectLSB=1
DisableResetExclusive=1
MapSelect=2
PatchMap=Patch88-PRO

[Force88ProMap]
comment=バンクセレクトLSBとGS-RESETを無視して、８８ＰＲＯＭＡＰに初期化
MIDI=C:88PROMAP W:20 C:BANKMSB0 W:20 C:PC0 W:20
DisableBankSelectLSB=1
DisableResetExclusive=1
MapSelect=3

[Force8850Map]
comment=バンクセレクトLSBとGS-RESETを無視して、８８５０ＭＡＰに初期化
MIDI=C:8850MAP W:20 C:BANKMSB0 W:20 C:PC0 W:20
DisableBankSelectLSB=1
DisableResetExclusive=1
MapSelect=4

[X5DBankA]
comment=X5D Bank A切替
MIDI=C:BANKMSB0

[PB0]
comment=PitchBend=0
MIDI=C:PB0

[88PRO-88Patch]
comment=８８ＰＲＯ＝＞８８へのパッチ
PatchMap=Patch88-PRO
PatchDrumMap=Patch88ProDrum

[88-55mk2Patch]
comment=８８マップを５５ｍｋ２へ変換
MIDI_A=C:55MAP W:20 C:PC0 W:20
PatchMap=Patch88mk2
PatchDrumMap=PatchKICKSNARE PatchCYMBALCLAP

[CMtoGS]
comment=ＣＭ－６４からＧＳに変換 Bank127,126使用
MIDI=M:CMtoGSEmu
DisableBankSelectLSB=1
DisableBankSelectMSB=1
DisableExclusive=1

[GStoGMEmu]
comment=ＧＳ音源をＧＭ音源に変換 SB32などに使える
GSToGMEmu=1
DisableExclusive=1

[XGtoGMEmu]
comment=ＸＧ音源をＧＭ音源に変換 SB32などに使える
XGToGMEmu=1
DisableExclusive=1

[XGtoGSEmu]
comment=ＸＧ音源をＧＳ音源に変換 GSなどに使える
XGToGSEmu=1
DisableExclusive=1
PatchDrumMap=PatchDrumXGtoGS

[127BankEmu]
comment=ＳＣ－５５のバンク１２７、１２６をＧＳでエミュレート
MIDI_A=C:55Map
PatchMap=Patch127BankEmu Patch126BankEmu Patch07Card

[126BankEmu]
comment=ＳＣ－５５のバンク１２６をＧＳでエミュレート
;MIDI_A=C:55Map
PatchMap=Patch126BankEmu Patch07Card

[07Card]
comment=07Card
PatchMap=Patch07Card

[NS5RInit]
comment=NS5Rの初期化
MIDI_A=$F0 $42 $30 $42 $12 $00 $00 $7C $00 $F7 W:50 $F0 $42 $30 $42 $00 $01 $00 $F7 W:50 $F0 $42 $30 $42 $01 $00 $F7 W:50 $F0 $42 $30 $42 $12 $00 $00 $04 $7F $F7

[Mode05R/W]
comment=NS5Rの設定 (05R/W)
MIDI_A=$F0 $42 $30 $42 $01 $01 $F7 W:50

[GStoXG]
comment=GS=>XGの変換
DisableBankSelectLSB=1
GSToXGEmu=1
PatchDrumMap=PatchKICKSNARE PatchCYMBALCLAP

[GStoX5D]
GSToX5DEmu=1

[XGtoX5D]
XGToX5DEmu=1

[Wait500]
MIDI_A=W:500

[88ProSetting]
;88ProSetting=1

[PIANOSOLO]
PatchMap=Patch:ALLPIANO

[MUSICBOX]
PatchMap=Patch:ALLMB
MIDI_A=C:PC10

[CHURCH]
PatchMap=Patch:ALLCHURCH
MIDI_A=C:PC19

[HARMONICA]
PatchMap=Patch:ALLHARMONICA
MIDI_A=C:PC22

[SD90Init]
comment=Init SD90 instruments in native mode
MIDI=C:SD90InitMap W:20 C:PC0 W:20
; must use disable reset sysex or it's stuck in GS mode
DisableResetExclusive=1

[SD90EmuSC88Pro]
comment=Use Edirol SD-90 to emulate Roland SC-88Pro
DisableExclusive=1
PatchMap=PatchSD90


